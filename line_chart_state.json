{
    "config": {
        "view": {
            "stroke": null
        }
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 200,
    "data": {
        "url": "https://raw.githubusercontent.com/haanhoang/Assignment-2/main/Data/data_line_chart_gambling_state.csv"
    },
    "mark": "area",
    "encoding": {
        "x": {
            "field": "Year",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "sum",
            "field": "Expenditure",
            "type": "quantitative"
        },
        "color": {
            "field": "State",
            "scale": {
                "scheme": "category20b"
            }
        }
    }
}